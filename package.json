    "meteor-node-stubs": "~0.2.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "react-router": "^3.0.0",
    "redux": "^3.6.0",
    "redux-act": "^1.1.0"
  },
  "devDependencies": {
    "eslint": "^3.11.1",
    "eslint-import-resolver-meteor": "^0.3.4",
    "eslint-plugin-jsx-a11y": "^3.0.1",
    "eslint-plugin-meteor": "^4.0.1",
    "eslint-plugin-react": "^5.0.1",
    "madge": "^1.4.3"
  }
}
